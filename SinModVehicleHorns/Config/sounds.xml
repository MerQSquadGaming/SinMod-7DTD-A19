<SinModVehicleHorns>
	<append xpath="/Sounds">

		<!-- Credit for this sound goes to http://soundbible.com/292-Bicycle-Bell-Ringing.html-->
		<SoundDataNode name="sinmodBikeBell">
			<AudioSource name="Sounds/AudioSource_SmallHorn"/>
			<Noise ID="4" range="30" volume="60" time="3" heat_map_strength="0.2" heat_map_time="90"/>
			<AudioClip ClipName="#@modfolder:Resources/sinmod-HornAudio.unity3d?bicycleHorn"/>
			<LocalCrouchVolumeScale value="1.0"/>
			<CrouchNoiseScale value="1"/>
			<NoiseScale value="1"/>
			<MaxVoices value="3"/>
			<MaxEntities name="6"/>
			<MaxRepeatRate value="0.01"/>
		</SoundDataNode>


