<SinModAnimalTraps>
    <append xpath="/blocks">
        <block name="trapChickenEmpty">
            <property name="Material" value="Mwood_weak"/>
            <property name="Shape" value="ModelEntity"/>
            <property name="Texture" value="282"/>
            <property name="Model" value="Entities/OutdoorDecor/classicDogHousePrefab"/>
            <property name="Path" value="solid"/>
            <property name="Place" value="TowardsPlacerInverted"/>
            <property name="ImposterExchange" value="imposterBlock" param1="2"/>
            <property name="CustomIcon" value="decoClassicDogHouse"/>
            <property name="CustomIconTint" value="ffd500"/>
            <property name="StabilitySupport" value="false"/>
            <property class="UpgradeBlock">
                <property name="ToBlock" value="trapChicken" />
                <property name="Item" value="ammoBait" />
                <property name="ItemCount" value="5" />
                <property name="UpgradeHitCount" value="1" />
            </property>
            <property class="RepairItems">
                <property name="resourceWood" value="10"/>
            </property>
            <property event="Destroy" name="trapChickenEmpty" count="1"/>
            <property name="FilterTags" value="fdecor,fother"/>
            <property name="UnlockedBy" value="trapAnimalSchematic"/>
            <property name="DescriptionKey" value="trapChickenEmptyDesc"/>
            <property name="DisplayInfo" value="Description"/>
        </block>
        <block name="trapChicken">
            <property name="Material" value="Mwood_weak"/>
            <property name="Shape" value="ModelEntity"/>
