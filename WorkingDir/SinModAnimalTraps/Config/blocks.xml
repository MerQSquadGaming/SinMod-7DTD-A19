            <property name="StabilitySupport" value="false"/>
            <property name="FilterTags" value="fdecor,fother"/>
            <property name="Class" value="PlantGrowing"/>
            <property name="PlantGrowing.Next" value="trapChickenFull"/>
            <property name="PlantGrowing.GrowthRate" value="2"/>
            <!-- setting 1 is erratic. Recommend 2+ for testing or use -->
            <property name="PlantGrowing.FertileLevel" value="0"/>
            <property name="PlantGrowing.IsRandom" value="false"/>
            <property name="DescriptionKey" value="trapChickenDesc"/>
            <property name="DisplayInfo" value="Description"/>
